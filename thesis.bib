	Issn = {0956-7976},
	Journal = {Psychological science},
	Keywords = {Affect, Defense Mechanisms, Female, Humans, Male, Motivation, Time Factors, Verbal Behavior, Vocabulary},
	Month = feb,
	Note = {{PMID:} 18271855},
	Number = {2},
	Pages = {103--108},
	Shorttitle = {Errors are aversive},
	Title = {Errors are aversive: defensive motivation and the error-related negativity},
